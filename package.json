{
  "name": "@bmd-studio/genstack-container-postgraphile",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "setup": "yarn install",
    "start": "node -r ts-node/register src/index.ts",
    "start:debug": "yarn start --inspect=0.0.0.0:${DEFAULT_DEBUG_PORT:-9229}",
    "start:prod": "node dist/index.js",
    "start:pm2": "pm2-runtime pm2.config.js",
    "start:docker": "yarn build && yarn dc:dev up",
    "start:docker-prod": "yarn dc up",
    "dc": "docker-compose --env-file=./env/defaults -f docker-compose.yml -f docker-compose.standalone.yml --compatibility",
    "dc:dev": "cross-env NODE_ENV=development GS_ENV=development yarn dc -f docker-compose.development.yml",
    "test": "yarn test:integration",
    "test:integration": "jest --testPathPattern=integration/",
    "test:container": "cross-env TEST_MODE=container yarn test:integration",
    "build": "tsc",
    "build:watch": "yarn build --watch",
    "build:docker": "yarn dc:dev build",
    "build:docker-prod": "yarn dc build",
    "clean": "rimraf dist/ && rimraf coverage/ && rimraf node_modules/"
  },
  "dependencies": {
    "@bmd-studio/genstack-hooks": "1.0.5",
    "@bmd-studio/graphql-mqtt-subscriptions": "1.4.2",
    "@fullstackio/postgraphile-upsert-plugin": "1.4.0",
    "@graphile-contrib/pg-many-to-many": "1.0.1",
    "@graphile-contrib/pg-order-by-related": "1.0.0-beta.6",
    "@graphile-contrib/pg-simplify-inflector": "6.1.0",
    "@graphile/pg-aggregates": "0.1.0",
    "@graphile/subscriptions-lds": "4.12.1",
    "body-parser": "1.19.0",
    "chalk": "4.1.1",
    "change-case": "4.1.2",
    "connect-pg-simple": "5.0.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "debug": "4.3.1",
    "dotenv-parse-variables": "1.0.1",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "graphile-column-privileges-mutations": "0.0.9",
    "graphql-subscriptions": "1.2.1",
    "helmet": "4.6.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "mqtt": "4.2.6",
    "pg": "8.6.0",
    "postgraphile": "4.12.2",
    "postgraphile-plugin-connection-filter": "1.1.3",
    "postgraphile-plugin-nested-mutations": "1.1.0",
    "sift": "8.5.1",
    "upath": "1.2.0"
  },
  "devDependencies": {
    "@apollo/client": "3.3.19",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.10",
    "@types/debug": "4.1.5",
    "@types/dotenv-parse-variables": "0.2.0",
    "@types/jest": "26.0.23",
    "@types/jsonwebtoken": "8.5.1",
    "@types/lodash": "4.14.170",
    "@types/morgan": "1.9.2",
    "@types/node": "14.17.2",
    "@types/node-fetch": "2.5.10",
    "@types/pg": "8.6.0",
    "@types/uuid": "8.3.0",
    "cross-env": "7.0.3",
    "get-port": "5.1.1",
    "graphql": "15.5.0",
    "graphql-request": "3.4.0",
    "http-status-codes": "2.1.4",
    "jest": "26.6.3",
    "pm2": "5.1.0",
    "rimraf": "3.0.2",
    "testcontainers": "7.11.1",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.3.2",
    "uuid": "8.3.2"
  },
  "resolutions": {
    "graphql": "15.5.0"
  }
}
