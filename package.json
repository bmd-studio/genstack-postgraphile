{
  "name": "@bmd-studio/genstack-postgraphile",
  "version": "4.0.16",
  "description": "Postgraphile extended with authentication, MQTT PubSub and more.",
  "main": "dist/index.js",
  "scripts": {
    "setup": "yarn install && yarn setup:deduplicate",
    "setup:deduplicate": "yarn yarn-deduplicate yarn.lock",
    "start": "node -r ts-node/register src/start.ts",
    "start:debug": "yarn start --inspect=0.0.0.0:${DEFAULT_DEBUG_PORT:-9229}",
    "start:prod": "node dist/start.js",
    "start:pm2": "pm2-runtime pm2.config.js",
    "start:docker": "yarn build && yarn dc:dev up",
    "start:docker-prod": "yarn dc up",
    "dc": "docker-compose --env-file=./env/defaults -f docker-compose.yml -f docker-compose.standalone.yml --compatibility",
    "dc:dev": "cross-env NODE_ENV=development yarn dc -f docker-compose.development.yml",
    "test": "yarn test:integration",
    "test:integration": "jest --testPathPattern=integration/",
    "test:container": "cross-env TEST_MODE=container yarn test:integration",
    "build": "tsc",
    "build:watch": "yarn build --watch",
    "build:docker": "yarn dc:dev build",
    "build:docker-prod": "yarn dc build",
    "publish:patch": "npm version patch",
    "publish:npm": "yarn build && yarn test && npm shrinkwrap && npm publish --access public",
    "publish:dry": "yarn publish:npm --dry-run",
    "clean": "rimraf dist/ && rimraf coverage/ && rimraf node_modules/"
  },
  "keywords": [
    "Genstack",
    "Postgraphile",
    "GraphQL",
    "Postgres"
  ],
  "homepage": "https://github.com/bmd-studio/genstack-postgraphile",
  "bugs": {
    "url": "https://github.com/bmd-studio/genstack-postgraphile/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bmd-studio/genstack-postgraphile.git"
  },
  "author": "BMD Studio <hello@bmd.studio> (https://bmd.studio/)",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "env/defaults"
  ],
  "dependencies": {
    "@bmd-studio/graphql-mqtt-subscriptions": "1.4.3",
    "@graphile-contrib/pg-many-to-many": "1.0.2",
    "@graphile-contrib/pg-order-by-related": "1.0.0",
    "@graphile-contrib/pg-simplify-inflector": "6.1.0",
    "@graphile/pg-aggregates": "0.1.1",
    "@graphile/subscriptions-lds": "4.13.0",
    "body-parser": "1.20.2",
    "chalk": "4.1.2",
    "change-case": "4.1.2",
    "connect-pg-simple": "5.0.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "dotenv-parse-variables": "1.0.1",
    "express": "4.19.2",
    "express-session": "1.18.0",
    "graphile-column-privileges-mutations": "0.0.9",
    "graphql-subscriptions": "1.2.1",
    "helmet": "4.6.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "morgan": "1.10.0",
    "mqtt": "4.3.8",
    "pg": "8.11.3",
    "postgraphile": "4.13.0",
    "postgraphile-plugin-connection-filter": "1.1.3",
    "postgraphile-plugin-nested-mutations": "1.1.0",
    "postgraphile-upsert-plugin": "3.3.0",
    "sift": "8.5.1",
    "upath": "1.2.0"
  },
  "devDependencies": {
    "@apollo/client": "3.9.9",
    "@types/cookie-parser": "1.4.7",
    "@types/cors": "2.8.17",
    "@types/debug": "4.1.12",
    "@types/dotenv-parse-variables": "0.2.0",
    "@types/jest": "26.0.24",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash": "4.17.0",
    "@types/morgan": "1.9.9",
    "@types/node": "14.18.63",
    "@types/node-fetch": "2.6.11",
    "@types/pg": "8.11.4",
    "@types/uuid": "8.3.4",
    "cross-env": "7.0.3",
    "get-port": "5.1.1",
    "graphql": "15.8.0",
    "graphql-request": "3.7.0",
    "http-status-codes": "2.3.0",
    "jest": "26.6.3",
    "pm2": "5.3.1",
    "rimraf": "3.0.2",
    "testcontainers": "7.24.0",
    "ts-jest": "26.5.6",
    "ts-node": "10.9.2",
    "typescript": "4.9.5",
    "uuid": "8.3.2",
    "yarn-deduplicate": "^6.0.0"
  },
  "resolutions": {
    "graphql": "15.8.0"
  }
}
